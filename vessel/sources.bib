  journal  = {Vacuum},
  volume   = {45},
  number   = {10},
  pages    = {1059-1061},
  year     = {1994},
  note     = {Special Issue Proceedings of the 1st European Topical Conference On Hard Coatings and the II Iberian Vacuum Meeting 12-15 July 1993, Alicante, Spain},
